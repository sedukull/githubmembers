{
  "name": "xendit-app-org-members",
  "version": "1.0.0",
  "description": "API to retrieve the org members. Members are retrieved based upon the followers count in descening order.",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=development jest",
    "test-watch": "npm test -- --watch",
    "start": "node app.js"
  },
  "jest": {
    "verbose": true,
    "notify": true,
    "collectCoverage": true,
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "roots": [
      "<rootDir>/__tests__"
    ]
  },
  "keywords": [
    "Express",
    "RestAPI"
  ],
  "author": "Santhosh",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-list-endpoints": "^4.0.1",
    "loglevel": "*",
    "util": "*"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "jest": "25.1.0",
    "nodemon": "^1.19.1",
    "supertest": "4.0.2"
  }
}
